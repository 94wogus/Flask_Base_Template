
{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "<aws id num>.dkr.ecr.ap-northeast-2.amazonaws.com/<ECR name>:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "<container port>"
    }
  ],
  "Volumes": [
    {
      "ContainerDirectory": "/var/app",
      "HostDirectory": "/var/app"
    }
  ],
  "Logging": "/var/eb_log"
}